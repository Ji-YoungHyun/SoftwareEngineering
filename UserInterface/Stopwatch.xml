        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="42dp"
        android:layout_height="102dp"
        android:text=":"
        android:textAlignment="center"
        android:textSize="70"
        android:textStyle="bold"
