        tools:layout_editor_absoluteY="218dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="81dp"
        android:layout_height="102dp"
        android:text="00"
        android:textAlignment="center"
        android:textSize="70"
        android:textStyle="bold"
